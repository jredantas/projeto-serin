<?xml version="1.0"?>
<!DOCTYPE rdf:RDF [
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
    <!ENTITY serin "http://www.activeontology.com.br/serin.owl#" >
    <!ENTITY location "http://www.unifor.br/vehicle.owl" >
]>
<rdf:RDF
	xmlns:xsd="&xsd;"
	xmlns:rdfs="&rdfs;"
	xmlns:rdf="&rdf;"
	xmlns:owl="&owl;"
 	xmlns:serin="&serin;"
 	xmlns:vel="&location;#"
	xml:base="&location;">

	<owl:Ontology rdf:about="&location;">
		<owl:imports rdf:resource="&serin;" />
	</owl:Ontology>

	<!-- Brand Property
	 (http://www.unifor.br/Vehicle.owl#brand) -->
	<owl:FunctionalProperty rdf:ID="brand">
		<rdfs:domain rdf:resource="#Vehicle" />
		<rdfs:range rdf:resource="&xsd;string" />
		<serin:put/>
		<serin:get/>
		<serin:post/>
		<serin:list/>
		<serin:delete/>
	</owl:FunctionalProperty>

	<!-- Model Property
	 (http://www.unifor.br/Vehicle.owl#model) -->
	<owl:FunctionalProperty rdf:ID="model">
		<rdfs:domain rdf:resource="#Vehicle" />
		<rdfs:range rdf:resource="&xsd;string" />
		<serin:put/>
		<serin:get/>
		<serin:post/>
		<serin:list/>
		<serin:delete/>
	</owl:FunctionalProperty>

	<!-- http://www.unifor.br/Vehicle.owl#Vehicle -->
	<owl:Class rdf:ID="Vehicle">
		<serin:put/>
		<serin:get/>
		<serin:post/>
		<serin:list/>
		<serin:delete/>
	</owl:Class>
</rdf:RDF>